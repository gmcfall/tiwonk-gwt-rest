<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.0.rc1//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.5.0.rc1/distro-source/core/src/gwt-module.dtd">
<module rename-to="example.person">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name="org.tiwonk.gwt.rest.GwtRest" />
	<source path="client"/>
	
	<entry-point class="org.tiwonk.rest.example.person.client.PersonEntryPoint" />
	
	<generate-with
		class="org.tiwonk.gwt.rest.generator.ClientResourceGenerator">
		<when-type-assignable
			class="org.tiwonk.gwt.rest.client.ClientResource" />
	</generate-with>
</module>
